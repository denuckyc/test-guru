<%= form_with model: [:admin, @test], local: true do |form| %>
  <%= render 'shared/errors', resource: @test %>
    <p>
      <%= form.label :title %>
      <%= form.text_field :title %>
    </p>
    <p>
      <%= form.label :category_id %>
      <%= form.collection_select :category_id, Category.all, :id, :title, include_blank: false %>
    </p>

    <%= form.submit %>
<% end %>
<br>
